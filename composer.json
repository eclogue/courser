{
  "name": "racecourse/courser",
  "description": "light and faster php rest api lib",
  "version": "0.0.2",
  "license": "MIT",
  "authors": [
    {
      "name": "bugbear",
      "email": "mulberry10th@gmail.com",
      "homepage": "http://bugbear.me"
    }
  ],
  "autoload": {
    "psr-4": {
      "Courser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories": {
    "0": {
      "type": "vcs",
      "url": "https://github.com/racecourse/courser"
    },
    "1": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    },
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    }
  },
  "require": {
    "php": ">=7.0.0",
    "lcobucci/jwt": "^3.2",
    "pimple/pimple": "^3.0",
    "squizlabs/php_codesniffer": "^3.0",
    "psr/http-message": "^1.0"
  },
  "replace": {
    "courser/http": "self.version"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "phpcs": "php vendor/bin/phpcs",
    "test": "php vendor/bin/phpunit --coverage-text"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1"
  }
}
