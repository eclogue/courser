{
  "name": "eclogue/courser",
  "description": "light and faster php rest api lib",
  "version": "0.0.2",
  "license": "MIT",
  "authors": [
    {
      "name": "bugbear",
      "email": "mulberry10th@gmail.com",
      "homepage": "https://github.com/eclogue/courser"
    }
  ],
  "autoload": {
    "psr-4": {
      "Courser\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "https://packagist.phpcomposer.com"
    },
    "1": {
      "type": "vcs",
      "url": "http://github.com/eclogue/bulrush"
    },
    "2": {
      "type": "vcs",
      "url": "http://github.com/eclogue/hayrick"
    }
  },
  "require": {
    "php": ">=7.0.0",
    "pimple/pimple": "^3.0",
    "psr/http-message": "^1.0",
    "eclogue/bulrush": "dev-master",
    "eclogue/hayrick": "dev-master",
    "php-di/php-di": "dev-master"
  },
  "replace": {
    "courser/http": "self.version"
  },
  "minimum-stability": "dev",
  "prefer-stable": false,
  "scripts": {
    "phpcs": "php vendor/bin/phpcs",
    "test": "php vendor/bin/phpunit --coverage-text",
    "cgi-server": "php -t example -S 127.0.0.1:8179",
    "swoole-server": "php example/swoole.php"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.1",
    "squizlabs/php_codesniffer": "^3.0",
    "satooshi/php-coveralls": "dev-master"
  }
}
