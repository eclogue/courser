{
    "name": "racecourse/courser",
    "description": "light and faster php rest api lib",
    "version": "0.0.2",
    "license": "MIT",
    "authors": [
        {
            "name": "bugbear",
            "email": "mulberry10th@gmail.com",
            "homepage": "http://bugbear.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Courser\\": "src/"
        }
    },
    "repositories": {
        "0": {
            "type": "vcs",
            "url": "https://github.com/racecourse/courser"
        },
        "1": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        },
        "packagist": {
            "type": "composer",
            "url": "https://packagist.phpcomposer.com"
        }
    },
    "require": {
        "lcobucci/jwt": "^3.2",
        "pimple/pimple": "^3.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "minimum-stability": "stable",
    "scripts": {
        "phpcs": "php vendor/bin/phpcs",
        "test": "php vendor/bin/phpunit"
    },
    "require-dev": {
        "php-mock/php-mock": "^2.0",
        "php-mock/php-mock-phpunit": "^2.0"
    }
}
